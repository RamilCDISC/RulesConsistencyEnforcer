    "Core": {
        "Id": "CORE-000165",
        "Status": "Published",
        "Version": "1"
    },
    "Description": "Raise an error when --TPTREF is not present in dataset when --RFTDTC is present.",
    "Executability": "Fully Executable",
    "Outcome": {
        "Message": "--TPTREF is not present when --RFTDTC is present in a dataset",
        "Output_Variables": [
            "--RFTDTC",
            "--TPTREF"
        ]
    },
    "Rule_Type": "Record Data",
    "Scope": {
        "Classes": {
            "Include": [
                "ALL"
            ]
        },
        "Domains": {
            "Include": [
                "ALL"
            ]
        }
    },
    "Sensitivity": "Dataset"

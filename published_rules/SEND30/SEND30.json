    "Check": {
        "all": [
            {
                "name": "variable_name",
                "operator": "not_matches_regex",
                "value": "^[A-Z]*$"
            }
        ]
    },
    "Core": {
        "Id": "CORE-000569",
        "Status": "Published",
        "Version": "1"
    },
    "Description": "Raise an error when variable name is not in uppercase",
    "Executability": "Fully Executable",
    "Outcome": {
        "Message": "Variable name must be in uppercase"
    },
    "Rule_Type": "Variable Metadata Check",
    "Scope": {

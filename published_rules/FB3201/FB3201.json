    "Check": {
        "all": [
            {
                "name": "--DY",
                "operator": "not_exists"
            },
            {
                "name": "--DTC",
                "operator": "exists"
            }
        ]
    },
    "Core": {
        "Id": "CORE-000321",
        "Status": "Published",
        "Version": "1"
    },
    "Description": "Raise an error when Date/Time of Collection (--DTC) is present in the dataset but Study Day of Visit/Collection/Exam (--DY) is not present.",
    "Executability": "Fully Executable",
    "Outcome": {
        "Message": "The Study Day of Visit/Collection/Exam (--DY) is not present in the dataset when Date/Time of Collection (--DTC) is present."
    },
    "Rule_Type": "Record Data",
    "Scope": {
        "Classes": {
            "Include": [
                "ALL"
            ]
        },
        "Domains": {
            "Include": [
                "ALL"
            ]
        }
    },
    "Sensitivity": "Dataset"
}
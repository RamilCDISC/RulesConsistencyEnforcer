    "Check": {
        "all": [
            {
                "name": "variable_label",
                "operator": "not_matches_regex",
                "value": "^\\s*[A-Z]\\S*\\s*((a|an|the|and|per|to|but|or|for|in|of|on|with|([A-Z]\\S*))\\s*)*$"
            }
        ]
    },
    "Core": {
        "Id": "CORE-000594",
        "Status": "Published",
        "Version": "1"
    },
    "Description": "Raise an error when a variable label is not in title case",
    "Executability": "Fully Executable",
    "Outcome": {
        "Message": "Variable label is not in title case."
    },
    "Rule_Type": "Variable Metadata Check",
    "Scope": {
        "Classes": {
            "Include": [
                "ALL"
            ]
        },
        "Domains": {
            "Include": [
                "ALL"
            ]
        },
        "Use Case": "INDH, PROD, NONCLIN"
    },
    "Sensitivity": "Record"
}
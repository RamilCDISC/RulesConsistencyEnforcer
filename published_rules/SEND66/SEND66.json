    "Core": {
        "Id": "CORE-000588",
        "Status": "Published",
        "Version": "1"
    },
    "Description": "Variables that include dates (--DTC) are required to be presented according to the international standard ISO 8601 date or date/time format",
    "Executability": "Fully Executable",
    "Outcome": {
        "Message": "Variable value is not in ISO 8601 date / datetime format",
        "Output_Variables": [
            "--DTC"
        ]
    },

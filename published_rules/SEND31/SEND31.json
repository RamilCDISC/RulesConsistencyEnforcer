    "Check": {
        "all": [
            {
                "name": "DOMAIN",
                "operator": "not_matches_regex",
                "value": "^[A-Z].*"
            }
        ]
    },
    "Core": {
        "Id": "CORE-000519",
        "Status": "Published",
        "Version": "1"
    },
    "Description": "Raise an error when the value of DOMAIN has a character other than A-Z as the first character",
    "Executability": "Fully Executable",
    "Outcome": {
        "Message": "First character of DOMAIN value must be in the range A-Z",
        "Output_Variables": [
            "DOMAIN"
        ]
    },
    "Rule_Type": "Record Data",
    "Scope": {
        "Classes": {
            "Include": [
                "ALL"
            ]
        },
        "Domains": {
            "Include": [
                "ALL"
            ]
        }
    },
    "Sensitivity": "Record"
}